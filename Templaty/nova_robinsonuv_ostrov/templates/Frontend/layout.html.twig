<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>{% block title %}Robinson{% endblock %}</title>
        
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('frontend/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('frontend/css/font-awesome.min.css') }}">
            <link rel="stylesheet" href="{{ asset('frontend/css/style.css') }}">
        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('frontend/js/jquery.min.js') }}"></script>
            <script src="{{ asset('frontend/js/jquery-ui/jquery-ui.min.js') }}"></script>
            <script src="{{ asset('frontend/js/jquery-ui/jquery.ui.touch-punch.min.js') }}"></script>
            <script src="{{ asset('frontend/js/bootstrap.min.js') }}"></script>
            <script>$.widget.bridge('uibutton', $.ui.button);</script>
            <script src="{{ asset('frontend/js/app.js') }}"></script>
        {% endblock %}
    </head>
    <body>
        {% include 'Frontend/nova-header.html.twig' %}
        
        <div class="container">
            {% include 'Frontend/header.html.twig' %}
            {% block content %}{% endblock %}
            {% include 'Frontend/footer.html.twig' %}
        </div>
    </body>
</html>
